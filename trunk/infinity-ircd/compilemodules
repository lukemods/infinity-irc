echo "InfinityIRCd is now installing the modules."
mv m_sendraw.c src/modules
mv m_banlink.c src/modules
mv m_ircops.c src/modules
mv nocolorumode.c src/modules
mv m_privdeaf.c src/modules
mv textban.c src/modules
mv noctcp.c src/modules
make custommodule MODULEFILE=m_sendraw
make custommodule MODULEFILE=m_banlink
make custommodule MODULEFILE=m_ircops
make custommodule MODULEFILE=nocolorumode
make custommodule MODULEFILE=m_privdeaf
make custommodule MODULEFILE=textban
make custommodule MODULEFILE=noctcp
echo "Installation Completed. Type ./unreal start to start the IRCd."